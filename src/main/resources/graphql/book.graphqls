type Query{
    books: [Book]
    getBookById(id: ID): Book
}

type Mutation{
    createBook(name: String, pageCount: Int):Book
    createBookFull(book: BookIn):Book
    updateBook(id: ID, name: String, pageCount:Int):Book
    deleteBook(id: ID):Book
}

input BookIn{
    name: String
    pageCount: Int
}

type Book {
    id: ID
    name: String
    pageCount: Int
}